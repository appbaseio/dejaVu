{
    "name":"dejavu",
    "version":"1.7.0",
    "icons":{
        "16":"live/assets/img/icon_16.png",
        "32":"live/assets/img/icon_32.png",
        "48":"live/assets/img/icon_48.png",
        "128":"live/assets/img/icon_128.png"
    },
    "background": {
        "scripts": [ "background.js" ]
    },
    "web_accessible_resources": [
        "live/index.html"
    ],
    "permissions": ["storage", "system.display", "<all_urls>", "https://appbaseio.auth0.com/*"],
    "manifest_version": 2,
    "offline_enabled":true,
    "browser_action": {
        "default_icon": "live/assets/img/icon_32.png",
        "default_title": "Dejavu"
    },
    "description": "The missing Web UI for Elasticsearch.",
    "incognito": "split",
    "author": "Farhan Chauhan, appbase.io"
}
